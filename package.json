{
  "name": "cdk-aspects-examples",
  "version": "0.1.0",
  "bin": {
    "index": "bin/cdk-aspects-examples.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "check packages": "npm-check-updates -u",
    "purge cdk.out": "rm -rvf cdk.out",
    "purge modules": "rm -rvf node_modules",
    "purge lockfile": "rm package-lock.json",
    "purge cache": "npm cache clean -f",
    "render extended diagram": "npx cdk ls -q && npx cdk-dia --collapse=false && rm -rvf diagram.dot && mv -v diagram.png ./images/extended_diagram.png"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "20.11.5",
    "@types/prettier": "2.7.3",
    "aws-cdk": "2.123.0",
    "esbuild": "0.19.12",
    "npm-check-updates": "16.14.12",
    "package-json": "9.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.123.0",
    "cdk-dia": "0.10.0",
    "cdk-nag": "^2.28.18",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
